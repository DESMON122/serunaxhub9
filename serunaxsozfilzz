    end
end
end) -- Args(<String> Name, <Function> Callback)
toggle:Set(false) -- Args(<Boolean> ToggleState, <Function?> Callback)

local toggle = section:Toggle("Anti AFK", function(bool)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/peatchXD/Roblox-Script-AFK/main/Script%20AFK.lua"))()
end) -- Args(<String> Name, <Function> Callback)
toggle:Set(false) -- Args(<Boolean> ToggleState, <Function?> Callback)

local section = tab:Section("Misc") -- Args(<string> Name)

local title = section:Title("Menu") -- Args(<string> Name)
title:ChangeText("Menu") -- Args(<String> NewText)

local toggle = section:Toggle("Remove Fog", function(bool)
    game.Lighting.FogEnd = 100000
game.Lighting.FogStart = 0
game.Lighting.ClockTime = 14
game.Lighting.Brightness = 2
game.Lighting.GlobalShadows = false
end) -- Args(<String> Name, <Function> Callback)
